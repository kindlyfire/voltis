{
    "name": "frontend",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "engines": {
        "node": "^20.19.0 || >=22.12.0"
    },
    "scripts": {
        "dev": "vite",
        "build": "run-p type-check \"build-only {@}\" --",
        "preview": "vite preview",
        "build-only": "vite build",
        "type-check": "vue-tsc --build",
        "format": "prettier --write \"src/**/*.{vue,ts}\" *.json *.ts index.html"
    },
    "dependencies": {
        "@mdi/font": "^7.4.47",
        "@tailwindcss/vite": "^4.1.17",
        "@tanstack/vue-query": "^5.92.0",
        "@unhead/vue": "^2.0.19",
        "@vueuse/core": "^14.1.0",
        "dompurify": "^3.3.1",
        "pinia": "^3.0.4",
        "tailwindcss": "^4.1.17",
        "vue": "^3.5.25",
        "vue-router": "^4.6.3",
        "vuetify": "^3.11.0",
        "zod": "^4.1.13"
    },
    "devDependencies": {
        "@tsconfig/node24": "^24.0.3",
        "@types/node": "^24.10.1",
        "@vitejs/plugin-vue": "^6.0.2",
        "@vue/tsconfig": "^0.8.1",
        "npm-run-all2": "^8.0.4",
        "prettier": "^3.7.2",
        "typescript": "~5.9.0",
        "vite": "^7.2.4",
        "vite-plugin-vue-devtools": "^8.0.5",
        "vue-tsc": "^3.1.5"
    },
    "prettier": {
        "printWidth": 100,
        "singleQuote": true,
        "semi": false,
        "useTabs": false,
        "tabWidth": 4,
        "arrowParens": "avoid",
        "endOfLine": "lf",
        "trailingComma": "es5"
    }
}
